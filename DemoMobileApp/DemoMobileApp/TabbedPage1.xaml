<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage
    
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:local="clr-namespace:DemoMobileApp"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DemoMobileApp.TabbedPage1">
    <!--Pages can be added as references or inline-->
    <local:MasterDetailContactPage Title="Contacts" Icon="clock.png" ></local:MasterDetailContactPage>
    <NavigationPage Title="Tab 2" Icon="clock.png">
        <x:Arguments>
            <local:ContactPage/>
        </x:Arguments>

    </NavigationPage>


    <ContentPage Title="Tab 3" Icon="clock.png" >
        <Label Text="Tab2"></Label>
    </ContentPage>
</TabbedPage>